{
  "name": "pet-shop",
  "version": "0.0.0",
  "scripts": {
    "build:client": "ng build client",
    "config:decode": "node ./bin/config-decode.js",
    "config:encode": "node ./bin/config-encode.js",
    "hook:pre-commit": "run-p pretty-quick lint:all",
    "lint": "ng lint --fix",
    "lint:all": "npm run lint && npm run lint:scss",
    "lint:scss": "stylelint \"./src/**/*.scss\" --fix",
    "prepare": "npx husky install && npx husky set .husky/pre-commit \"npm run hook:pre-commit\"",
    "prettier": "prettier --write \"src/**/*.*(ts|js|json|html)\"",
    "pretty-quick": "pretty-quick --staged --pattern \"src/**/*.*(ts|js|json|html)\"",
    "start:client": "ng serve client --port=56753",
    "start:client:ssl": "ng serve client --ssl --port=56753",
    "test:client": "ng test client",
    "watch:client": "ng build client --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "class-transformer": "^0.5.1",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.set": "^4.3.2",
    "lodash.transform": "^4.6.0",
    "ngx-infinite-scroll": "^15.0.0",
    "ngx-pagination": "^6.0.3",
    "ngx-toastr": "^16.0.1",
    "reset-css": "^5.0.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.set": "^4.3.7",
    "@types/lodash.transform": "^4.6.7",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "stylelint": "^14.16.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-declaration-strict-value": "^1.9.1",
    "stylelint-no-unsupported-browser-features": "^6.0.1",
    "stylelint-scss": "^4.3.0",
    "tslint-config-prettier": "^1.18.0"
  }
}
